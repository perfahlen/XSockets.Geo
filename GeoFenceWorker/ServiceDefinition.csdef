    <ConfigurationSettings>
      <Setting name="uri" />
      <Setting name="origin" />
    </ConfigurationSettings>
    <Endpoints>
      <InputEndpoint name="geofence" protocol="tcp" port="8080" localPort="8080" />
    </Endpoints>
  </WorkerRole>
</ServiceDefinition>